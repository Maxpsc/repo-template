{
  "name": "{{pkgName}}",
  "version": "0.0.0",
  "description": "{{desc}}",
  "keywords": [],
  "main": "dist/{{pkgName}}.cjs.js",
  "module": "dist/{{pkgName}}.esm.js",
  "bin": {
    "{{pkgName}}": "bin/{{pkgName}}.js"
  },
  "types": "dist/{{pkgName}}.d.ts",
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "dev": "node scripts/dev.js",
    "build": "node scripts/build.js",
    "lint": "eslint 'src/**/*.@(js|ts)' --fix",
    "format": "prettier --write 'src/**/*.@(js|ts)'",
    "test": "npm run test:once -- --watch",
    "test:once": "jest --runInBand --colors --forceExit",
    "coverage": "codecov",
    "prepare": "husky install",
    "release": "pnpm build && release"
  },
  "dependencies": {
    "chalk": "^4.1.1",
    "commander": "^7.2.0",
    "og-toolkit": "^0.0.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@eljs/release": "0.7.3",
    "@rollup/plugin-commonjs": "^21.0.2",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-replace": "^4.0.0",
    "@types/fs-extra": "^9.0.3",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.21",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "chalk": "^4.1.2",
    "codecov": "^3.8.3",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-dirs": "^0.0.4",
    "eslint-plugin-prettier": "^4.2.1",
    "execa": "^5.1.1",
    "fs-extra": "^10.1.0",
    "husky": "^8.0.1",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.4",
    "minimist": "^1.2.5",
    "prettier": "^2.7.1",
    "prettier-plugin-organize-imports": "^3.0.3",
    "rollup": "^2.77.2",
    "rollup-plugin-typescript2": "^0.32.1",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "homepage": "https:////#readme",
  "bugs": {
    "url": "https://///issues"
  },
  "lint-staged": {
    "{bin,src,__tests__}/**/*.{js,ts}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "author": "{{author}}",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "license": "MIT"
}
